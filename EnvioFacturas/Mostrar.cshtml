@{
    if (Session["IdUsuario"] == null)
    {
        Response.Redirect("~/Default");
    }
    
    String IdFactura = Request.QueryString["IdFactura"];
    String PDFOXML = Request.QueryString["Tipo"];

    Facturas Fac = new Facturas();
    String Redirige = Fac.Redireccionar(PDFOXML, IdFactura);
    
    if(Redirige.Length != 0)
    {
        Response.Redirect("~/" + Redirige);
    }
}
